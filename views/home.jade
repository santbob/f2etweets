#chatarea.row.display
    h3= title
    .eight.columns(style="text-align:left;")
    if data.css
        div#css
                H2 CSS Tweets
                for tweet in data.css
                        div=tweet.text
    if data.js
        div#js
                H2 JS Tweets
                for tweet in data.js
                        div=tweet.text

    if data.html
        div#html
                H2 Html Tweets
                for tweet in data.html
                        div=tweet.text
